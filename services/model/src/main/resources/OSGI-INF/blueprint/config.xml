<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">    

    <bean id="factory" class="org.woz.protozoa.model.FactoryImpl"/>

    <bean id="locationservice"
          class="org.woz.protozoa.services.model.LocationServiceImpl">
        <property name="factory" ref="factory"/>
    </bean>
        
    <service ref="locationservice" interface="org.woz.protozoa.services.model.LocationService">
        <bean class="org.woz.protozoa.services.model.LocationServiceImpl"/>
    </service>
    
</blueprint>